{
  "manifest_version": 3,
  "name": "leo's extension",
  "version": "1.0.0",
  "content_scripts": [
    {
      "matches": ["https://*/*"],
      "js": ["content_scripts/content.js"],
      "permissions": ["activeTab", "scripting"]
    }
  ],
  "background": {
    "service_worker": "service/service.js"
  }
}
